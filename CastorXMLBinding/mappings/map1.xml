<?xml version="1.0" ?>

<mapping>
	<class name="demos.xml.castor.Employee">
	       <map-to xml="StaffMember"/>
	       
	       <field name="name" type="string">
	       		<bind-xml name="identity" node="element"/>
	       </field>
	       <field name="age" type="integer">
	       		<bind-xml name="years" node="attribute"/>
	       </field>
	       <field name="salary" type="double">
	       		<bind-xml name="identity" node="attribute"/>
	       </field>
	</class>
</mapping>